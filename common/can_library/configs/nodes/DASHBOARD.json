{
    "node_name": "DASHBOARD",
    "scheduler": "freertos",
    "busses": {
        "VCAN": {
            "peripheral": "FDCAN2",
            "tx": [
                {
                    "msg_name": "filt_throttle_brake",
                    "msg_desc": "Filtered throttle and brake values",
                    "signals": [
                        {
                            "sig_name": "throttle",
                            "sig_desc": "Official throttle request from driver",
                            "type": "uint16_t",
                            "length": 12,
                            "scale": 0.02442,
                            "unit": "%"
                        },
                        {
                            "sig_name": "brake",
                            "sig_desc": "Processed brake request from driver",
                            "type": "uint16_t",
                            "length": 12,
                            "scale": 0.02442,
                            "unit": "%"
                        }
                    ],
                    "msg_period": 15,
                    "msg_priority": 2
                },
                {
                    "msg_name": "start_button",
                    "msg_desc": "Car start button pressed",
                    "signals": [
                        {
                            "sig_name": "start",
                            "type": "bool"
                        }
                    ],
                    "msg_period": 0,
                    "msg_priority": 1
                },
                {
                    "msg_name": "cooling_driver_request",
                    "msg_desc": "Cooling Pump/Fan requests from Dash LCD",
                    "signals": [
                        {
                            "sig_name": "dt_pump",
                            "type": "uint8_t"
                        },
                        {
                            "sig_name": "dt_fan",
                            "type": "uint8_t",
                            "unit": "%",
                            "min": 0,
                            "max": 100
                        },
                        {
                            "sig_name": "batt_pump",
                            "type": "uint8_t"
                        },
                        {
                            "sig_name": "batt_pump2",
                            "type": "uint8_t"
                        },
                        {
                            "sig_name": "batt_fan",
                            "type": "uint8_t",
                            "unit": "%",
                            "min": 0,
                            "max": 100
                        }
                    ],
                    "msg_period": 0,
                    "msg_priority": 3
                },
                {
                    "msg_name": "dashboard_mcu_temp",
                    "msg_desc": "periodic dashboard MCU temp",
                    "signals": [
                        {
                            "sig_name": "mcu_temp",
                            "type": "int16_t"
                        }
                    ],
                    "msg_period": 4000,
                    "msg_priority": 4
                },
                {
                    "msg_name": "dashboard_start_logging",
                    "msg_desc": "Log enable request for DAQ SD card",
                    "signals": [
                        {
                            "sig_name": "logging_enabled",
                            "sig_desc": "Request SD card logging to begin",
                            "type": "bool"
                        }
                    ],
                    "msg_period": 0,
                    "msg_priority": 3
                },
                {
                    "msg_name": "dash_can_stats",
                    "msg_desc": "CAN diagnostics",
                    "signals": [
                        {
                            "sig_name": "can_tx_overflow",
                            "type": "uint8_t",
                            "sig_desc": "CAN Tx queue overflow count"
                        },
                        {
                            "sig_name": "can_tx_fail",
                            "type": "uint8_t",
                            "sig_desc": "CAN Tx fail count (due to timeout in mailbox)"
                        },
                        {
                            "sig_name": "can_rx_overflow",
                            "type": "uint8_t",
                            "sig_desc": "CAN Rx queue overflow count"
                        },
                        {
                            "sig_name": "can_rx_overrun",
                            "type": "uint8_t",
                            "sig_desc": "CAN Rx FIFO overrun count"
                        }
                    ],
                    "msg_period": 1000,
                    "msg_priority": 4
                },
                {
                    "msg_name": "dash_version",
                    "msg_desc": "Dashboard version",
                    "signals": [
                        {
                            "sig_name": "git_hash",
                            "type": "uint32_t",
                            "sig_desc": "int representation of git short hash"
                        }
                    ],
                    "msg_priority": 5,
                    "msg_period": 5000
                },
                {
                    "msg_name": "LWS_Config",
                    "msg_desc": "Steering sensor configuration",
                    "signals": [
                        {
                            "sig_name": "CCW",
                            "type": "uint8_t",
                            "length": 3
                        },
                        {
                            "sig_name": "Reserved_1",
                            "type": "uint8_t",
                            "length": 5
                        },
                        {
                            "sig_name": "Reserved_2",
                            "type": "uint8_t",
                            "length": 8
                        }
                    ],
                    "msg_period": 0,
                    "msg_id_override": "0x7C0",
                    "msg_priority": 3
                }
            ],
            "rx": [
                {
                    "msg_name": "main_hb"
                },
                {
                    "msg_name": "rear_motor_currents_volts"
                },
                {
                    "msg_name": "orion_info"
                },
                {
                    "msg_name": "orion_currents_volts"
                },
                {
                    "msg_name": "orion_errors"
                },
                {
                    "msg_name": "max_cell_temp"
                },
                {
                    "msg_name": "precharge_hb"
                },
                {
                    "msg_name": "rear_wheel_speeds"
                },
                {
                    "msg_name": "coolant_out"
                },
                {
                    "msg_name": "gearbox"
                },
                {
                    "msg_name": "sdc_status"
                },
                {
                    "msg_name": "gps_speed"
                }
            ]
        }
    },
    "fault_library_enabled": true,
    "generate_fault_messages": true,
    "faults": [
        {
            "fault_name": "APPS_wiring_t1",
            "max": 3700,
            "min": 300,
            "priority": "error",
            "time_to_latch": 10,
            "time_to_unlatch": 1000,
            "lcd_message": "APPS Wiring Fail T1"
        },
        {
            "fault_name": "APPS_wiring_t2",
            "max": 3700,
            "min": 300,
            "priority": "error",
            "time_to_latch": 10,
            "time_to_unlatch": 1000,
            "lcd_message": "APPS Wiring Fail T2"
        },
        {
            "fault_name": "BSE",
            "max": 1,
            "min": 0,
            "priority": "error",
            "time_to_latch": 100,
            "time_to_unlatch": 1000,
            "lcd_message": "Brake Wiring Fail (BSE)"
        },
        {
            "fault_name": "bspd",
            "max": 182,
            "min": 0,
            "priority": "error",
            "time_to_latch": 100,
            "time_to_unlatch": 1000,
            "lcd_message": "BSE Wiring Fail B2"
        },
        {
            "fault_name": "Implaus_detected",
            "max": 4095,
            "min": -4095,
            "priority": "error",
            "time_to_latch": 10,
            "time_to_unlatch": 100,
            "lcd_message": "APPS Implaus Detected"
        },
        {
            "fault_name": "APPS_brake",
            "max": 1,
            "min": 0,
            "priority": "warning",
            "time_to_latch": 100,
            "time_to_unlatch": 100,
            "lcd_message": "Release Both Pedals"
        }
    ]
}
