{
    "node_name": "DASHBOARD",
    "scheduler": "psched",
    "busses": {
        "VCAN": {
            "peripheral": "CAN1",
            "tx": [
                {
                    "msg_name": "raw_throttle_brake",
                    "msg_desc": "Throttle and brake values",
                    "signals": [
                        {
                            "sig_name": "throttle",
                            "sig_desc": "Raw sensor reading from left throttle sensor",
                            "type": "uint16_t",
                            "length": 12,
                            "unit": "%"
                        },
                        {
                            "sig_name": "throttle_right",
                            "sig_desc": "Raw sensor reading from right throttle sensor",
                            "type": "uint16_t",
                            "length": 12,
                            "unit": "%"
                        },
                        {
                            "sig_name": "brake",
                            "sig_desc": "Raw sensor reading from left (front) brake sensor",
                            "type": "uint16_t",
                            "length": 12,
                            "unit": "%"
                        },
                        {
                            "sig_name": "brake_right",
                            "sig_desc": "Raw sensor reading from right (rear) brake sensor",
                            "type": "uint16_t",
                            "length": 12,
                            "unit": "%"
                        },
                        {
                            "sig_name": "brake_pot",
                            "sig_desc": "Reading for potentiometer on brake pedal [OLD, no longer used]",
                            "type": "uint16_t",
                            "length": 12,
                            "unit": "%"
                        }
                    ],
                    "msg_period": 15,
                    "msg_priority": 3
                },
                {
                    "msg_name": "shock_front",
                    "msg_desc": "Shockpot readings from front left and front right",
                    "signals": [
                        {
                            "sig_name": "left_shock",
                            "sig_desc": "Travel distance of front left shockpot",
                            "type": "int16_t",
                            "scale": 0.1,
                            "unit": "mm"
                        },
                        {
                            "sig_name": "right_shock",
                            "sig_desc": "Travel distance of front right shockpot",
                            "type": "int16_t",
                            "scale": 0.1,
                            "unit": "mm"
                        }
                    ],
                    "msg_period": 15,
                    "msg_priority": 3
                },
                {
                    "msg_name": "load_sensor_readings_dash",
                    "msg_desc": "Load Sensor normal force readings from front left and right",
                    "signals": [
                        {
                            "sig_name": "left_load_sensor",
                            "type": "float",
                            "unit": "N"
                        },
                        {
                            "sig_name": "right_load_sensor",
                            "type": "float",
                            "unit": "N"
                        }
                    ],
                    "msg_period": 15,
                    "msg_priority": 3
                },
                {
                    "msg_name": "cooling_driver_request",
                    "msg_desc": "Cooling Pump/Fan requests from Dash LCD",
                    "signals": [
                        {
                            "sig_name": "dt_pump",
                            "type": "uint8_t"
                        },
                        {
                            "sig_name": "dt_fan",
                            "type": "uint8_t",
                            "unit": "%",
                            "min": 0,
                            "max": 100
                        },
                        {
                            "sig_name": "batt_pump",
                            "type": "uint8_t"
                        },
                        {
                            "sig_name": "batt_pump2",
                            "type": "uint8_t"
                        },
                        {
                            "sig_name": "batt_fan",
                            "type": "uint8_t",
                            "unit": "%",
                            "min": 0,
                            "max": 100
                        }
                    ],
                    "msg_period": 0,
                    "msg_priority": 3
                },
                {
                    "msg_name": "filt_throttle_brake",
                    "msg_desc": "Filtered throttle and brake values",
                    "signals": [
                        {
                            "sig_name": "throttle",
                            "sig_desc": "Official throttle request from driver",
                            "type": "uint16_t",
                            "length": 12,
                            "scale": 0.02442,
                            "unit": "%"
                        },
                        {
                            "sig_name": "brake",
                            "sig_desc": "Processed brake request from driver",
                            "type": "uint16_t",
                            "length": 12,
                            "scale": 0.02442,
                            "unit": "%"
                        }
                    ],
                    "msg_period": 15,
                    "msg_priority": 3
                },
                {
                    "msg_name": "start_button",
                    "msg_desc": "Car start button pressed",
                    "signals": [
                        {
                            "sig_name": "start",
                            "type": "uint8_t",
                            "length": 1
                        }
                    ],
                    "msg_period": 0,
                    "msg_priority": 3
                },
                {
                    "msg_name": "dashboard_mcu_temp",
                    "msg_desc": "periodic dashboard MCU temp",
                    "signals": [
                        {
                            "sig_name": "mcu_temp",
                            "type": "int16_t"
                        }
                    ],
                    "msg_period": 4000,
                    "msg_priority": 3
                },
                {
                    "msg_name": "dashboard_voltage",
                    "msg_desc": "Voltage rail measurements on dashboard",
                    "signals": [
                        {
                            "sig_name": "volts_3v3",
                            "type": "uint16_t",
                            "scale": 0.01,
                            "unit": "V"
                        },
                        {
                            "sig_name": "volts_5v",
                            "type": "uint16_t",
                            "scale": 0.01,
                            "unit": "V"
                        },
                        {
                            "sig_name": "volts_12v",
                            "type": "uint16_t",
                            "scale": 0.01,
                            "unit": "V"
                        },
                        {
                            "sig_name": "volts_24v",
                            "type": "uint16_t",
                            "scale": 0.01,
                            "unit": "V"
                        }
                    ],
                    "msg_period": 5000,
                    "msg_priority": 3
                },
                {
                    "msg_name": "dashboard_vcu_parameters",
                    "msg_desc": "periodic dashboard vcu parameters",
                    "signals": [
                        {
                            "sig_name": "vcu_fmode",
                            "type": "uint8_t",
                            "length": 1
                        },
                        {
                            "sig_name": "vcu_cmode",
                            "type": "uint8_t",
                            "length": 1
                        },
                        {
                            "sig_name": "vt_db_val",
                            "type": "uint8_t"
                        },
                        {
                            "sig_name": "tv_pp_val",
                            "type": "uint16_t"
                        },
                        {
                            "sig_name": "tc_tr_val",
                            "type": "uint8_t"
                        },
                        {
                            "sig_name": "vs_max_sr_val",
                            "type": "uint8_t"
                        }
                    ],
                    "msg_period": 500,
                    "msg_priority": 3
                },
                {
                    "msg_name": "dashboard_start_logging",
                    "msg_desc": "Log enable request for DAQ SD card",
                    "signals": [
                        {
                            "sig_name": "logging_enabled",
                            "sig_desc": "Request SD card logging to begin",
                            "type": "uint8_t",
                            "length": 1
                        }
                    ],
                    "msg_period": 0,
                    "msg_priority": 3
                },
                {
                    "msg_name": "dash_can_stats",
                    "msg_desc": "CAN diagnostics",
                    "signals": [
                        {
                            "sig_name": "can_tx_overflow",
                            "type": "uint8_t",
                            "sig_desc": "CAN Tx queue overflow count"
                        },
                        {
                            "sig_name": "can_tx_fail",
                            "type": "uint8_t",
                            "sig_desc": "CAN Tx fail count (due to timeout in mailbox)"
                        },
                        {
                            "sig_name": "can_rx_overflow",
                            "type": "uint8_t",
                            "sig_desc": "CAN Rx queue overflow count"
                        },
                        {
                            "sig_name": "can_rx_overrun",
                            "type": "uint8_t",
                            "sig_desc": "CAN Rx FIFO overrun count"
                        }
                    ],
                    "msg_period": 1000,
                    "msg_priority": 3
                },
                {
                    "msg_name": "dash_version",
                    "msg_desc": "Dashboard version",
                    "signals": [
                        {
                            "sig_name": "git_hash",
                            "type": "uint32_t",
                            "sig_desc": "int representation of git short hash"
                        }
                    ],
                    "msg_priority": 5,
                    "msg_period": 5000
                }
            ],
            "rx": [
                {
                    "msg_name": "main_hb"
                },
                {
                    "msg_name": "rear_motor_currents_volts"
                },
                {
                    "msg_name": "orion_info"
                },
                {
                    "msg_name": "orion_errors"
                },
                {
                    "msg_name": "max_cell_temp"
                },
                {
                    "msg_name": "precharge_hb"
                },
                {
                    "msg_name": "rear_wheel_speeds"
                },
                {
                    "msg_name": "coolant_temps"
                },
                {
                    "msg_name": "coolant_out",
                    "callback": true
                },
                {
                    "msg_name": "gearbox"
                },
                {
                    "msg_name": "dashboard_bl_cmd",
                    "callback": true
                },
                {
                    "msg_name": "INVB_CRIT"
                }
            ]
        }
    },
    "generate_fault_messages": true,
    "faults": [
        {
            "fault_name": "APPS_wiring_t1",
            "max": 3700,
            "min": 300,
            "priority": "error",
            "time_to_latch": 10,
            "time_to_unlatch": 1000,
            "lcd_message": "APPS Wiring Fail T1"
        },
        {
            "fault_name": "APPS_wiring_t2",
            "max": 3700,
            "min": 300,
            "priority": "error",
            "time_to_latch": 10,
            "time_to_unlatch": 1000,
            "lcd_message": "APPS Wiring Fail T2"
        },
        {
            "fault_name": "BSE",
            "max": 1,
            "min": 0,
            "priority": "error",
            "time_to_latch": 100,
            "time_to_unlatch": 1000,
            "lcd_message": "Brake Wiring Fail (BSE)"
        },
        {
            "fault_name": "bspd",
            "max": 182,
            "min": 0,
            "priority": "error",
            "time_to_latch": 100,
            "time_to_unlatch": 1000,
            "lcd_message": "BSE Wiring Fail B2"
        },
        {
            "fault_name": "Implaus_detected",
            "max": 4095,
            "min": -4095,
            "priority": "error",
            "time_to_latch": 10,
            "time_to_unlatch": 100,
            "lcd_message": "APPS Implaus Detected"
        },
        {
            "fault_name": "APPS_brake",
            "max": 1,
            "min": 0,
            "priority": "warning",
            "time_to_latch": 100,
            "time_to_unlatch": 100,
            "lcd_message": "Release Both Pedals"
        }
    ]
}
