{
    "node_name": "DRIVELINE",
    "scheduler": "freertos",
    "busses": {
        "VCAN": {
            "peripheral": "FDCAN2",
            "tx": [
                {
                    "msg_name": "front_shockpots",
                    "msg_desc": "Shockpot readings from front left and front right",
                    "signals": [
                        {
                            "sig_name": "left",
                            "sig_desc": "Travel distance of front left shockpot",
                            "type": "int16_t",
                            "scale": 0.1,
                            "unit": "mm"
                        },
                        {
                            "sig_name": "right",
                            "sig_desc": "Travel distance of front right shockpot",
                            "type": "int16_t",
                            "scale": 0.1,
                            "unit": "mm"
                        }
                    ],
                    "msg_period": 10,
                    "msg_priority": 5
                },
                {
                    "msg_name": "rear_shockpots",
                    "msg_desc": "Shockpot readings from rear left and rear right",
                    "signals": [
                        {
                            "sig_name": "left",
                            "sig_desc": "Left Shockpot Travel",
                            "type": "int16_t",
                            "scale": 0.1,
                            "unit": "mm"
                        },
                        {
                            "sig_name": "right",
                            "sig_desc": "Right Shockpot Travel",
                            "type": "int16_t",
                            "scale": 0.1,
                            "unit": "mm"
                        }
                    ],
                    "msg_period": 10,
                    "msg_priority": 5
                },
                {
                    "msg_name": "front_driveline_version",
                    "msg_desc": "Front Driveline Version",
                    "signals": [
                        {
                            "sig_name": "git_hash",
                            "type": "uint32_t",
                            "sig_desc": "int representation of git short hash"
                        }
                    ],
                    "msg_priority": 5,
                    "msg_period": 5000
                },
                {
                    "msg_name": "rear_driveline_version",
                    "msg_desc": "Rear Driveline Version",
                    "signals": [
                        {
                            "sig_name": "git_hash",
                            "type": "uint32_t",
                            "sig_desc": "int representation of git short hash"
                        }
                    ],
                    "msg_priority": 5,
                    "msg_period": 5000
                }
            ],
            "rx": []
        }
    },
    "fault_library_enabled": false,
    "generate_fault_messages": false,
    "faults": []
}
